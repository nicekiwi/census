{
    "name": "nicekiwi/census",
    "description": "Helper package to interact with the Planetside 2 Census service.",
    "type": "library",
    "keywords": [
        "census",
        "planetside2",
        "ps2"
    ],
    "scripts": {
        "test": "phpunit"
    },
    "require": {
        "php": "^8.1",
        "illuminate/config": "^9.0",
        "illuminate/support": "^9.0",
        "illuminate/http": "^9.0",
        "guzzlehttp/guzzle": "^7.5",
        "textalk/websocket": "^1.6",
        "symfony/http-client": "^6.1",
        "ext-simplexml": "*",
        "ext-json": "*",
        "ext-libxml": "*"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Nicekiwi\\Census\\": "src",
            "Nicekiwi\\Census\\Tests\\": "tests"
        }
    },
    "extra": {
      "laravel": {
        "providers": [
          "Nicekiwi\\Census\\Providers\\CensusServiceProvider"
        ],
        "aliases": {
          "ApiCilent": "Nicekiwi\\Census\\Facades\\ApiCilent",
          "StreamClient": "Nicekiwi\\Census\\Facades\\StreamClient"
        }
      }
    },
    "authors": [
        {
            "name": "Ezra Sharp",
            "email": "code@nice.kiwi"
        }
    ],
    "require-dev": {
        "orchestra/testbench": "^7.11",
        "laravel/pint": "^1.2",
        "phpstan/phpstan": "^1.9",
        "squizlabs/php_codesniffer": "^3.7",
        "phpunit/phpunit": "^9.5"
    }
}
