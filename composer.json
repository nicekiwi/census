{
    "name": "nicekiwi/census",
    "description": "Helper package to interact with the Planetside 2 Census service.",
    "type": "library",
    "require": {
        "illuminate/support": "^9.39",
        "illuminate/http": "^9.39",
        "textalk/websocket": "^1.6",
        "symfony/http-client": "^6.1",
        "ext-simplexml": "*",
        "ext-json": "*"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Nicekiwi\\Census\\": "src/Census",
            "Nicekiwi\\Census\\Tests\\": "tests"
        }
    },
    "extra": {
      "laravel": {
        "providers": [
          "Nicekiwi\\Census\\CensusServiceProvider"
        ],
        "aliases": {
          "Census": "Nicekiwi\\Census\\Facade"
        }
      }
    },
    "authors": [
        {
            "name": "Ezra Sharp",
            "email": "code@nice.kiwi"
        }
    ],
    "require-dev": {
        "pestphp/pest": "^1.22",
        "orchestra/testbench": "^7.11",
        "laravel/pint": "^1.2",
        "phpstan/phpstan": "^1.9"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
